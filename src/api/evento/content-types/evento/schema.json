{
  "kind": "collectionType",
  "collectionName": "eventos",
  "info": {
    "singularName": "evento",
    "pluralName": "eventos",
    "displayName": "Eventos"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Tipo": {
      "type": "enumeration",
      "enum": [
        "Roda de conversa",
        "Exposição",
        "Shows"
      ],
      "required": true,
      "private": true
    },
    "Nome": {
      "type": "string",
      "required": true
    },
    "Data": {
      "type": "date",
      "required": true
    },
    "Horario_Inicio": {
      "type": "time",
      "required": true
    },
    "Horario_Fim": {
      "type": "time",
      "required": true
    },
    "Local": {
      "type": "string",
      "required": true
    },
    "Preco": {
      "type": "decimal",
      "required": true
    },
    "Descricao": {
      "type": "text",
      "required": true
    },
    "URL_Inscricao": {
      "type": "string",
      "unique": true
    },
    "Foto_divulgacao": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": true
    },
    "Foto_Evento": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": true
    },
    "parceires": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::parceire.parceire",
      "mappedBy": "eventos"
    }
  }
}

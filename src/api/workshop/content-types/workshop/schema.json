{
  "kind": "collectionType",
  "collectionName": "workshop",
  "info": {
    "singularName": "workshop",
    "pluralName": "workshops",
    "displayName": "Oficinas",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "nome": {
      "type": "string",
      "required": true
    },
    "carga_horaria": {
      "type": "string"
    },
    "local": {
      "type": "string",
      "required": true
    },
    "url_inscricao": {
      "type": "string"
    },
    "foto_divulgacao": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "foto_oficina": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "data_inicio": {
      "type": "date",
      "required": true
    },
    "data_fim": {
      "required": true,
      "type": "date"
    },
    "horario_inicio": {
      "type": "string",
      "required": true
    },
    "horario_fim": {
      "type": "string",
      "required": true
    },
    "preco": {
      "type": "string"
    },
    "descricao": {
      "type": "richtext",
      "required": true
    },
    "tipo": {
      "type": "enumeration",
      "enum": [
        "Roda de Conversa",
        "Show"
      ],
      "required": true
    },
    "parceires": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::partner.partner",
      "inversedBy": "oficinas"
    }
  }
}

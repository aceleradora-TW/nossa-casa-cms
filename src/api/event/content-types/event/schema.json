{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Evento",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "nome": {
      "type": "string",
      "required": true
    },
    "tipo": {
      "type": "enumeration",
      "enum": [
        "Roda de Conversa",
        "Show",
        "Festival",
        "Encontro",
        "RolÃª",
        "Feira",
        "Jornada",
        "Oficinas",
        "Rodas",
        "Expo"
      ],
      "required": true
    },
    "local": {
      "type": "string"
    },
    "preco": {
      "type": "string"
    },
    "descricao": {
      "type": "richtext",
      "default": "## Titulo "
    },
    "url_inscricao": {
      "type": "string"
    },
    "foto_divulgacao": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "fotos_evento": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "videos"
      ]
    },
    "parceires": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::partner.partner",
      "inversedBy": "events"
    },
    "data_inicio": {
      "required": true,
      "type": "date"
    },
    "horario_inicio": {
      "type": "string",
      "required": true
    },
    "horario_fim": {
      "type": "string",
      "required": true
    },
    "destaque": {
      "type": "boolean"
    },
    "subeventos": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::event.event"
    },
    "data_fim": {
      "required": true,
      "type": "date"
    },
    "oficinas": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::workshop.workshop"
    }
  }
}
